<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SMS_BLPRR</display-name>
  <welcome-file-list>
    <welcome-file>SuppliesListing.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>supplycontrol</servlet-name>
  	<servlet-class>com.sms.servlet.SupplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>supplycontrol</servlet-name>
  	<url-pattern>/supplylistings</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>supplycontrol</servlet-name>
  	<url-pattern>/supplymaintenance</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>supplycontrol</servlet-name>
  	<url-pattern>/addsupplies</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>supplycontrol</servlet-name>
  	<url-pattern>/updatesupplies</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>supplycontrol</servlet-name>
  	<url-pattern>/searchsupplies</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>issueSupplies</servlet-name>
  	<servlet-class>com.sms.servlet.IssuedSupplyController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>issueSupplies</servlet-name>
  	<url-pattern>/issueSupplies</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>supplystock</servlet-name>
  	<servlet-class>com.sms.servlet.SupplyStockServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>supplystock</servlet-name>
  	<url-pattern>/supplystock</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>supplystock</servlet-name>
  	<url-pattern>/searchSupplyStock</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>supplystock</servlet-name>
  	<url-pattern>/saveSupplyStock</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>supplystock</servlet-name>
  	<url-pattern>/updateSupplyStock</url-pattern>
  </servlet-mapping>
</web-app>